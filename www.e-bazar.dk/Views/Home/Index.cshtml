@{
    ViewBag.Title = "Index";
}
<div class="seperator">

</div>
<div class="l-dx-filler"></div>
<div class="remove_desk_block col-md-12 z-padding-sides-0">

    <div class="col-xs-12 l-dx-welcome1 z-background-color-lightgrey z-border-darkgrey z-padding-top-15 z-padding-sides-0">
        <div class="col-xs-4 xz-only-desk">
            <i class="far fa-file-alt my_icon"></i>&nbsp;<a href="@Url.RouteUrl("Features")" class="z-black-link z-font-info">Features</a><br />
            <div class="z-tooltip1">
                <i class="far fa-file-alt"></i><span class="z-font-info">&nbsp;Infomation</span>
                <span class="z-desc-tooltip1 z-align-left">
                    @{
                        List<pageinfo> dlist = InfoHelper.GetInfo(5);
                        foreach (pageinfo i in dlist)
                        {
                            @i.date @:- @i.type:<br />
                            @i.text<br /><br />
                        }
                    }
                    @Html.RouteLink("Se alle", "Info")
                </span>
            </div>
        </div>
        <div class="col-xs-4">
            <div class="l-dx-welcome2 z-bold z-align-center">e-bazar.dk</div>
            <div class="z-align-center z-font-size-15">@SettingsHelper.Basic.COMMENT()</div>
        </div>
        
        @{

            string[] dmts = { "Jan", "Feb", "Mar", "Apr", "Maj", "Jun", "Jul", "Aug", "Sep", "Okt", "Nov", "Dec" };
            string dmon = dmts[DateTime.Now.Month - 1];
        }
        <div class="col-xs-4">

        <table class="l-dx-welcome3 xz-only-desk z-margin-left-auto">
            <tr><td><span class="z-color-green z-font-info">max besøg, @dmon</span></td><td><span class="z-font-info z-float-right">&nbsp;&nbsp;>&nbsp;@ViewBag.Stats.max_users_per_month&nbsp;</span></td></tr>
            <tr><td><span class="z-color-green z-font-info">besøgende i dag</span></td><td><span class="z-font-info z-float-right">&nbsp;&nbsp;>&nbsp;@ViewBag.Stats.users_per_day&nbsp;</span></td></tr>
            <tr><td><span class="z-font-info">antal stande</span></td><td><span class="z-font-info z-float-right">&nbsp;&nbsp;>&nbsp;@ViewBag.Stats.booths_count&nbsp;</span></td></tr>
            <tr><td><span class="z-font-info">antal varer</span></td><td><span class="z-font-info z-float-right">&nbsp;&nbsp;>&nbsp;@ViewBag.Stats.items_count&nbsp;</span></td></tr>
        </table>
        </div>
    </div>
</div>


