
@{
    ViewBag.Title = "ErrorPage";
}

<h2>Fejlside</h2>
<div>
    @if (string.IsNullOrEmpty(ViewBag.ErrorMessage))
    {
        <span>Der skete en fejl, beklager.<br /></span>
    }
    else
    {
        <span>@ViewBag.ErrorMessage </span><br />
        @*<span>@ViewBag.ErrorStack</span>*@
    }
    <br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
</div>
<script>
    $(document).ready(function () {
        $('.z-myalert').hide();
    });
</script>